import kr.ac.ctc.kopo35.Domain.ScoreItem;

public interface ScoreItemDao {
	public List<ScoreItem> selectAll(int pageNo, int CountPerPage, int SizePage);
	
	public ScoreItem selectOne(int id);
	
	public int insertScore(ScoreItem scoreItem);
	
	public int updateScore(ScoreItem scoreItem);
	
	public int deleteScore(ScoreItem scoreItem);
}
